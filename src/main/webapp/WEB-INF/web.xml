<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>NFTAddServlet</servlet-name>
        <servlet-class>com.t2308e.assignment.nftmanagement.servlet.NFTAddServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NFTAddServlet</servlet-name>
        <url-pattern>/addNFT</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NFTServlet</servlet-name>
        <servlet-class>com.t2308e.assignment.nftmanagement.servlet.NFTServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NFTServlet</servlet-name>
        <url-pattern>/nft</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NFTDeleteServlet</servlet-name>
        <servlet-class>com.t2308e.assignment.nftmanagement.servlet.NFTDeleteServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NFTDeleteServlet</servlet-name>
        <url-pattern>/deleteNFT</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>